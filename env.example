# API Configuration
REACT_APP_API_BASE_URL=https://api.example.com

# Application Configuration
REACT_APP_APP_NAME=React Boilerplate
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true

# External Services
REACT_APP_GOOGLE_ANALYTICS_ID=GA_TRACKING_ID
REACT_APP_SENTRY_DSN=YOUR_SENTRY_DSN

# Development
REACT_APP_ENABLE_MOCK_API=false
REACT_APP_MOCK_API_DELAY=1000

# Authentication
REACT_APP_AUTH_TOKEN_KEY=authToken
REACT_APP_REFRESH_TOKEN_KEY=refreshToken

# API Timeouts
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_RETRY_ATTEMPTS=3
REACT_APP_API_RETRY_DELAY=1000 